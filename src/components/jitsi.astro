<script src="/external_api.js" is:inline></script>

<script>
  addEventListener('DOMContentLoaded', async () => {

const meeting = await DyteClient.init({
        authToken: '',
        roomName: '',
        defaults: {
          audio: true,
          video: true,
        },
      });

      const meetingID:any = document.getElementById('meet') || {};
      meetingID['meeting'] = meeting;

    return () => {
      // Clean up the Jitsi Meet conference when the component is unmounted
    };
  });

</script>

<style>
  #meet {
    width: 80%;
    height: 600px;
  }
</style>

<dyte-meeting id="meet" class="">test</dyte-meeting>